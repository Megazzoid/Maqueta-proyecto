{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "allowJs": true,
    "outDir": "./dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "./clasesTS"
  ],
  "exclude": [
    "node_modules"
  ]
}
